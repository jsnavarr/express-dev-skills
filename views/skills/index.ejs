<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <title>Developer Skills</title>
</head>
<body>
    <h1>Developer Skills</h1>
    <ul>
      <% skills.forEach(function(s, idx) { %>
        <li>
            <a href="/skills/<%= idx %>"><h3><%= s.skill %></h3></a>
        </li>
      <% }); %>
    </ul>
    <a class="link-to-edit-add" href="/skills/new">Add Skill</a>
  </body>
</html>